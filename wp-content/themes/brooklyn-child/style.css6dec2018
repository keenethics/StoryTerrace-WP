    font-size: 24px;
    font-weight: bold;
    color: #333;
}
.packages-pricing .pricing-box .content-section a.learn-more {
    width: 140px;
    border-radius: 2px;
    border: solid 1px #f06d66;
    display: inline-block;
    font-size: 18px;
    color: #f06d66;
    text-decoration: none;
    padding: 8px 0;
    margin: 0 0 10px 0;
    transition: .3s ease-in-out;
}
