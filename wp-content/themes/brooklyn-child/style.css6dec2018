.ha-header{
  height: 78px;
  background-color: #ffffff !important;
  box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.1);
}
.ha-header .grid-container{
  padding-top:20px;
  padding-bottom:20px;
  padding-left: 0;
  padding-right: 0;
}
.ha-header .grid-20, .ha-header #navigation{
  padding: 0;
}
.ha-header .menu{
  float: right;
}
.ha-header .menu .menu-item-language, .ha-header .menu #wpmenucartli{
  display:none;
}
#header-section #navigation ul li + li {
    margin-left: 30px;
}
#header-section #navigation ul ul li + li{
  margin-left: 0;
}
#header-section #navigation ul li a{
  font-family: Circe;
  font-size: 18px !important;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: normal;
  text-align: left;
  color: #474f5a;
  position:relative;
}
#header-section #navigation ul li>a:after {
    content: "";
    height: 1px;
    position: absolute;
    display: inline-block;
    width: 100%;
    bottom: -3px;
    left: 0;
    right: 0;
    background-color: #474f5a;
    visibility: hidden;
}
#header-section #navigation ul li.current-menu-item>a:after, #header-section #navigation ul li>a:focus{
  visibility: visible;
}
#header-section #navigation ul li.current-menu-item>a, #header-section #navigation ul li.current-menu-item>a:hover, #header-section #navigation ul li>a:focus{
  color:#474f5a;
}
#header-section #navigation ul li a:hover, #header-section #navigation ul li a:focus{
  text-decoration: none;
}
#header-section #navigation ul ul li a{
  padding: 10px;
}
@media (min-width:1025px){
  #header-section #navigation ul ul{
    /*top:106%;*/
    margin-top: 0 !important;
    box-shadow:0px 5px 15px -2px rgba(0,0,0,0.15) !important;
    min-width:180px !important;
  }
  #header-section #navigation ul ul li a:hover, #header-section #navigation ul ul li a:focus{
    background-color: #f2f2f2 !important;
  }
}

.page-template-howitwork-tpl .rev_slider .slotholder:after {
    width: 100%;
    height: 100%;
    content: "";
    position: absolute;
    left: 0;
    top: 0;
