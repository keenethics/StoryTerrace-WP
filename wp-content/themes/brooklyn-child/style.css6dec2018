.packages-pricing{
  padding-top: 80px;
  padding-bottom: 100px;
  background-color: #f7f7f7;
}
h2.title, h2.title{
  font-size: 36px;
  text-align: center;
    font-weight: bold;
}
.packages-pricing h2.title {
    margin: 0 0 30px 0;
}
.packages-pricing .pricing-area {
    margin-top: 40px;
    margin-bottom: 10px;
    display: table;
    width: 94%;
    max-width: 1024px;
    margin-left: auto;
    margin-right: auto;
}
.title.center:after, .title:after {
    content: "";
    display: block;
    margin: 0 auto;
    width: 60px;
    padding-top: 10px;
    border-bottom: 2px solid #f06d66;
}
.packages-pricing p {
    font-size: 18px;
    line-height: 1.33;
    text-align: center;
    color: #414b56;
    margin: 0 0 20px 0;
}
.packages-pricing .pricing-box {
    border-radius: 4px;
    background-color: #fff;
    border: solid 1px #eaeaea;
    padding: 20px;
    transition: .5s ease-in-out;
}
.packages-pricing .pricing-box:hover {
    transition: .5s ease-in-out;
    box-shadow: 0 8px 16px 0 rgba(0,0,0,0.1);
    border: solid 1px #f3f3f3;
}
.packages-pricing .pricing-box:hover .content-section a.learn-more, .packages-pricing .pricing-box .content-section a.learn-more:focus {
    background: #f06d66;
    transition: .3s ease-in-out;
    color: #fff;
}
.packages-pricing .pricing-box .image-section {
    width: 110px;
    height: 110px;
    background-color: #e4e4e4;
    border-radius: 50%;
    margin: 0 auto 20px;
    overflow: hidden;
}
.packages-pricing .pricing-box .content-section{
  text-align: center;
}
.packages-pricing .pricing-box .content-section h1 {
    font-size: 26px;
    font-weight: bold;
    color: #414b56;
    margin: 0 0 20px 0;
    padding-bottom: 0;
    line-height: 1;
}
.packages-pricing .pricing-box .content-section h3 {
    font-size: 18px;
    line-height: 1.33;
    text-align: center;
    color: #414b56;
    font-weight: normal;
    margin: 0 0 15px 0;
}
.packages-pricing .pricing-box .content-section .price-list ul, .fa-ul {
    padding: 0;
    list-style: none;
    text-align: left;
    max-width: 210px;
    margin: 20px auto 10px;
}
.packages-pricing .pricing-box .content-section .price-list ul li, .fa-ul li {
    font-size: 14px;
    line-height: 1;
    text-align: left;
    color: #414b56;
    margin: 0 0 10px 0;
    position: relative;
    padding: 0 0 0 30px;
}
.packages-pricing .pricing-box .content-section .price-list ul li:before, .fa-ul li:before {
    content: "";
    background: url('images/check.png');
    position: absolute;
    left: 0;
    width: 20px;
    height: 20px;
    background-repeat: no-repeat;
}
.packages-pricing .pricing-box .content-section .price {
    text-transform: capitalize;
    font-size: 20px;
    margin: 0 0 15px 0;
}
